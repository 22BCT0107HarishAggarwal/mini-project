{
	"routes": [
		{
			"src": "/(.*).html",
			"dest": "/$1.html"
		},
		{
			"src": "/(.*).js",
			"dest": "/$1.js"
		},
		{
			"src": "/(.*).css",
			"dest": "/$1.css"
		},
		{
			"src": "/(.*).png",
			"dest": "/$1.png"
		},
		{
			"src": "/(.*).jpg",
			"dest": "/$1.jpg"
		},
		{
			"src": "/(.*).svg",
			"dest": "/$1.svg"
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	]
}
